N1

a, b, c = map(int, input().split(" "))

s = a+b+c

p = a*b*c

m = (a+b+c)/3

print(s, p ,m)


N2
import math

print ('введите координаты точки A')
x1 = float(input())
y1 = float(input())
print ('введите координаты точки B')
x2 = float(input())
y2 = float(input())
d = math.sqrt((y2-y1)**2 + (x2-x1)**2)
print('Ответ')
print(d)


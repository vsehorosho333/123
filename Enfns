N1 
n = int(input()) 
mlist = [i for i in range(1, n+1)] 
print(*mlist)

N2 
n, m = map(int, input().split()) 
lst = (range(n, m + n)) 
print(*lst)

N3 

n = int(input()) 
a = [i ** 2  for i in range(1,n+1)] 
print(a)

N4 
n = int( input("введите n:")) 
a = [] 
for i in range (1, n+1): 
    a.append (i) 
a.reverse() 
print (a) 
